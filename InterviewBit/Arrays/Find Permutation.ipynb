    "n = len(S)+1\n",
    "D = S.count('D')\n",
    "I = S.count('I')\n",
    "ans = []\n",
    "ic,dc = n-I+1,D\n",
    "ans+=[D+1]\n",
    "for i in S:                  #### main ####\n",
    "    if i=='I':\n",
    "        ans+=[ic]\n",
    "        ic+=1\n",
    "    else:\n",
    "        ans+=[dc]\n",
    "        dc-=1\n",
    "print(ans)"
