   "source": [
    "from math import factorial\n",
    "A = 'bac'\n",
    "n = len(A)\n",
    "B = sorted(A)\n",
    "ans = 1\n",
    "M = 1000003                          #### main #### Sorted Permutation Rank\n",
    "for i,c in enumerate(A):\n",
    "    ans+=B.index(c)*factorial(n-1-i)\n",
    "    ans%=M\n",
    "    B.remove(c)\n",
    "print(ans)"
   ]
  }
