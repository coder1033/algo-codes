    "A = \"A man, a plan, a canal: Panama\"\n",
    "s = ''\n",
    "l = 0\n",
    "for i in A:                                #### main #### Palindrome String\n",
    "    if i.isalnum():\n",
    "        s+=i.lower()\n",
    "        l+=1\n",
    "for i in range(l//2):\n",
    "    if s[i]!=s[l-i-1]:\n",
    "        print(0)\n",
    "        break\n",
    "else:\n",
    "    print(1)"
   ]
  }
