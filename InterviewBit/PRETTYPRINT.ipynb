    "A = 4\n",
    "\n",
    "ans = []\n",
    "n = A\n",
    "while A:                             #### main #### PRETTYPRINT\n",
    "    t = []\n",
    "    for i in range(n,A,-1):\n",
    "        t += [i]\n",
    "    ans+=[t+[A]*(A*2-1)+t[::-1]]\n",
    "    A-=1\n",
    "ans+=ans[:-1][::-1]\n",
    "print(*ans,sep='\\n')"
   ]
  }
